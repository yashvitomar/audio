{
  "android": {
    "activityName": "MainActivity",
    "deviceName": "Pixel_3_API_30",
    "packageName": "com.AudioNewsApp",
    "snapshotsPath": "./vqa-rn-mobile/snapshots/android",
    "dev": {
      "deviceParams": [
        "-no-audio",
        "-no-snapshot"
      ]
