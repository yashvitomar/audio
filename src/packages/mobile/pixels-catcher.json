  },
  "ios": {
    "port": 3010,
    "deviceName": "iPhone 11",
    "packageName": "org.reactjs.native.example.AudioNewsApp",
    "snapshotsPath": "./vqa-rn-mobile/snapshots/ios",
    "dev": {},
    "debug": {
      "appFile": "./vqa-rn-mobile/AudioNewsApp.app",
      "canStopDevice": true,
      "port": 3010
    }
  },
  "logLevelel": "v",
  "timeout": 600000
