{
  "name": "otp-input-confirmation",
  "version": "1.0.0",
  "repository": "https://gitlab.builder.ai/builder/builder-bx/Bx/blocks/react-native/development/react-native-master-app/packages/blocks/otp-input-confirmation.git",
  "license": "MIT",
  "scripts": {
    "test": "jest --verbose --coverage --passWithNoTests",
    "testBackend": "cd __tests__ && node endPointTest.js",
    "lint": "tsc && eslint 'src/**/*.{ts,tsx}' --fix",
    "sonar": "tsc && eslint -f json -o ./reports/report.json 'src/**/*.{ts,tsx}' && jest --verbose --coverage; sonar-scanner"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "eslint --fix",
      "prettier-eslint --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^25.1.3",
    "@typescript-eslint/eslint-plugin": "2.20.0",
    "@typescript-eslint/parser": "2.20.0",
    "babel-eslint": "10.0.3",
    "babel-jest": "25.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-prettier": "6.10.0",
    "eslint-config-react-app": "5.2.0",
    "eslint-plugin-babel": "5.3.0",
    "eslint-plugin-eslint-comments": "3.1.2",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-jest": "23.7.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-react": "7.18.3",
    "eslint-plugin-react-native": "3.8.1",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "jest-cucumber": "^2.0.11",
    "jest-enzyme": "7.1.2",
    "lint-staged": "10.0.7",
    "prettier-eslint-cli": "5.0.0",
    "raf": "^3.4.1",
    "ts-jest": "^25.2.0",
    "jest-sonar-reporter": "^2.0.0"
  },
  "dependencies": {
  },
  "jestSonar": {
    "reportPath": "reports",
    "reportFile": "test-reporter.xml",
    "indent": 4
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsConfig": "__tests__/tsconfig.base.json"
      }
    },
    "setupFilesAfterEnv": [
      "<rootDir>/node_modules/jest-enzyme/lib/index.js"
    ],
    "testResultsProcessor": "jest-sonar-reporter",
    "setupFiles": [
      "<rootDir>/test-setup.js"
    ],
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(js)$": "<rootDir>/node_modules/babel-jest",
      "\\.(ts|tsx)$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/",
      "<rootDir>/coverage/",
      "endPointTest.js"
    ],
    "cacheDirectory": ".jest/cache",
    "collectCoverageFrom": [
      "!babel.config.js",
      "!node_modules/**/*",
      "!build/**",
      "!node_modules/**/**/*",
      "!coverage/**/*"
    ]
  }
}
